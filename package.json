{
  "name": "drawrnajs",
  "description": "Visualizes RNA secondary structures.",
  "version": "1.2.4",
  "homepage": "https://github.com/bene200/drawrnajs",
  "author": {
    "name": "bene200",
    "email": "berau90@web.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bene200/drawrnajs.git"
  },
  "bugs": {
    "url": "https://github.com/bene200/drawrnajs/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/bene200/drawrnajs/blob/master/LICENSE"
    }
  ],
  "main": "src/drawrna.js",
  "scripts": {
    "test": "bun test",
    "build": "bun build src/drawrna.js --outfile build/drawrnajs.js --format esm --minify",
    "watch": "bun build src/drawrna.js --outfile build/drawrnajs.js --format esm --watch",
    "dev": "bun run build && bun --hot server.js",
    "prepublish": "bun run build"
  },
  "dependencies": {
    "cytoscape": "2.4.6",
    "jquery": "^2.1.3",
    "backbone": "^1.2.2",
    "cytoscape-edgehandles": "2.0.0",
    "underscore": "^1.3.8",
    "bootstrap": "^3.3.5",
    "tooltip": "^1.6.0"
  },
  "devDependencies": {},
  "sniper": {
    "js": [
      "/build/drawrnajs.js"
    ],
    "css": [
      "/style/bootstrap.css",
      "/style/spectrum.css",
      "/style/style.css"
    ],
    "snippets": [
      "examples"
    ],
    "first": "simple"
  },
  "keywords": [
    "rna",
    "secondary structure",
    "biojs",
    "galaxy-vis"
  ],
  "galaxy": {
    "datatypes": [
      "sequence.DotBracket"
    ]
  }
}
